  "scripts": {
    "build": "NODE_ENV=production gulp build-packages",
    "build-dev": "gulp build-packages",
    "pretest": "npm run lint && node test/pretest.js",
    "test": "mocha",
    "test-all": "npm test && npm run test-e2e",
    "test-unit": "node test/pretest.js && mocha",
    "test-e2e": "node test/e2e/server.js --env default --config ./test/e2e/nightwatch.json",
    "test-e2e-saucelabs": "if [ -n \"$SAUCE_ACCESS_KEY\" ]; then node test/e2e/server.js --env saucelabs-travis --config ./test/e2e/nightwatch.json; fi",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "watch": "watch 'clear && npm run lint' lib admin server test",
    "pretest-cov": "npm run clean && npm run lint",
    "current": "updtr",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "posttest-cov": "if [ -n \"$CODECLIMATE_REPO_TOKEN\" ]; then codeclimate-test-reporter < coverage/lcov.info; fi",
    "clean": "rimraf ./coverage"
  },
  "engines": {
    "node": ">= 0.12.0",
    "npm": ">= 3.0.0"
  }
